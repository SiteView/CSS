<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.csstudio.dct.dbexport.Record" table="dct_records">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="ioName" column="io_name" />
		<property name="epicsName" column="epics_name" />
		<property name="recordType" column="record_type" />
		<property name="dctId" column="dct_id" />
		<property name="dctProjectId" column="dct_project_id" />
		
	</class>
</hibernate-mapping>