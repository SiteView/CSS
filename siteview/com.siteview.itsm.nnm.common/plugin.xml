<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            id="com.siteview.itsm.nnm.util.actionSet"
            visible="true">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               icon="icons/sample.gif"
               toolbarPath="sampleGroup"
               label="&amp;Sample Action"
               menubarPath="sampleMenu/sampleGroup"
               class="com.siteview.itsm.nnm.scan.core.actions.ScanAction"
               id="com.siteview.itsm.nnm.util.actions.ScanAction"
               tooltip="Hello, Eclipse world">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.siteview.itsm.nnm.common.editor.ipResourceMgr.IpMacBaseDataEditor"
            default="false"
            id="com.siteview.itsm.nnm.common.ipMacBaseEdtior"
            name="IP-MAC基准数据">
      </editor>
      <editor
            class="com.siteview.itsm.nnm.common.editor.ipResourceMgr.SubNetMgrEditor"
            default="false"
            id="com.siteview.nnm.main.editor.subnetMrgEdit"
            name="子网设备列表">
      </editor>
   </extension>

</plugin>
